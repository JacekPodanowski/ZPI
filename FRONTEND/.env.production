# Production environment variables for Vite
NODE_ENV=production

# Disable unnecessary features in production
VITE_DEV_TOOLS=false

# API endpoints - MUST use HTTPS in production
VITE_API_BASE=https://api.youreasysite.pl
VITE_API_BASE_URL=https://api.youreasysite.pl/api/v1

# WebSocket URL (if needed)
VITE_WS_URL=wss://api.youreasysite.pl/ws

# Google OAuth Client ID
VITE_GOOGLE_CLIENT_ID=${VITE_GOOGLE_CLIENT_ID}

# Build configuration
VITE_APP_ROUTING_MODE=subdomain
